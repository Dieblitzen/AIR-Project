{
  "name": "refine_net_test3",
  "backbone": "resnet50",
  "backbone_kwargs": {},
  "pretrained": false,
  "refine_net_blocks":
    [
      ["layer4", "layer3"],
      ["layer2", "layer1"]
    ],
  "input_shape": [224, 224, 3],
  "classes":
    [
      "building:other",
      "highway:roads"
    ],
  "refine_net_kwargs": 
    {
      "reduce_channel_scale": 4,
      "rcu_kwargs": {},
      "mrf_kwargs": {},
      "crp_kwargs": {}
    },

  "augment":
  {
    "rotate_range":90,
    "flip":true, 
    "channel_shift_range":1e-10, 
    "multiplier":1, 
    "seed":0
  },

  "epochs": 200,
  "batch_size": 16,
  "loss": "BinaryCrossentropy",
  "loss_kwargs": 
    {
      "from_logits": true
    },
  "optimizer": "Adam",
  "optimizer_kwargs": 
    {
      "learning_rate":0.0001
    }
}